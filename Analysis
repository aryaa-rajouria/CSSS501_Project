
# ANALYSIS 

fdata <- read.csv('/Users/katherine/fdata2.csv')
options(scipen = 999)

# PRIMARY ANALYSIS

# MODEL 1AA: DOES EDS DECREASE SRH?

m1aa <- polr(as.factor(srh) ~ eds + age + gender + doc_status + income + married.LT + race + edu_highest
             + eng_read, data=fdata, method='probit')
summary(m1aa)
ci_m1aa <- confint(m1aa)

m1aa_res <- round(exp(cbind(OR = coef(m1aa), ci_m1aa)),3)
m1aa_res

MODEL 1A: WHAT FACTORS MAKE SOMEONE MORE LIKELY TO HAVE EDS?

m1a <- glm(eds ~ age + gender + doc_status + income_family + MARRIED + migrant_2 + race + edu_highest + dom_lang + eng_read, data=fdata, family=binomial)
mod_summary_sign <- summary(m1a)
exp(coef(m1a))


m1a_res <- round(cbind(exp(coef(m1a)), summary(m1a)$coefficients[,4]),3)
colnames(m1a_res) <- c('Odds', 'P-value')
m1a_res













